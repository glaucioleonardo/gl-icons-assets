{
  "name": "gl-icons-assets",
  "version": "1.5.0",
  "description": "Common icons to be used on web applications.",
  "assets": "assets",
  "main": "assets/img",
  "files": [
    "assets/*"
  ],
  "scripts": {
    "copy-files": "npm run copy-main && npm run copy-md",
    "copy-main": "copyfiles -a -u2 \"src/assets/img/**\" assets",
    "copy-md": "copyfiles -a -u1 \"src/README.md\" assets",
    "clean": "shx rm -rf assets",
    "production": "npm run clean && npm run copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glaucioleonardo/gl-icons-assets.git"
  },
  "keywords": [
    "svg",
    "image",
    "icons",
    "web-icons",
    "ng-icons",
    "angular styles"
  ],
  "author": "Gl√°ucio Leonardo Sant'ana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/glaucioleonardo/gl-icons-assets/issues"
  },
  "homepage": "https://github.com/glaucioleonardo/gl-icons-assets#readme",
  "devDependencies": {}
}
